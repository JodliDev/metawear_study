<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape>
			<solid android:color="#33000000"/>
		</shape>
	</item>
	<item>
		<rotate
			android:fromDegrees="160"
			android:toDegrees="0"
			android:pivotX="50%"
			android:pivotY="50%">
			<shape
				android:shape="line"
				android:top="1dip">
				<stroke
					android:width="2dp"
					android:color="#000"/>
			</shape>
		</rotate>
	</item>
</layer-list>